{
	"rootdir": "~/.dotfiles",
	"helpRoot": "/cli/assets",
	"ignorefiles": [".DS_Store","Caveats"],
	"message": {
		"error": {
			"char": "❗",
			"colour": "red",
			"sound": "bad.mp3"
